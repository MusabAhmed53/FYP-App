<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <FrameLayout android:id="@+id/frameStack"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_35pxv"
        android:layout_marginEnd="@dimen/_33pxh"
        android:layout_marginStart="@dimen/_19pxh"
        android:layout_marginTop="@dimen/_65pxv" android:layout_width="match_parent">
        <FrameLayout android:id="@+id/frameStackwelcomebilal"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_2">
            <LinearLayout android:id="@+id/linearColumnwelcomebilal"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout
                        android:id="@+id/linearRowwelcomebilal"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtWelcomeBilal"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.76"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@string/msg_welcome_instruc"
                            style="@style/txtPoppinsbold18" tools:text="@string/msg_welcome_instruc"/>
                        <LinearLayout android:id="@+id/linearRowsearch"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_84pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_weight="0.24"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageSearch"
                                android:layout_height="@dimen/_21pxh"
                                android:layout_width="@dimen/_21pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_search"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                            <ImageView android:id="@+id/imageMegaphone"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_21pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_megaphone"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_megaphone"/>
                        </LinearLayout>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <androidx.appcompat.widget.SearchView
                    android:background="@drawable/rectangle_bg_black_900_radius_20"
                    android:gravity="center_horizontal"
                    android:id="@+id/searchViewGroupOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_37pxv"
                    android:layout_width="match_parent"
                    android:theme="@style/searchViewGroupOne"
                    app:iconifiedByDefault="false"
                    app:queryBackground="@android:color/transparent"
                    app:queryHint="@string/lbl_search" app:searchIcon="@drawable/img_search_white_a700"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCourseinprogr"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/msg_course_in_progr"
                    style="@style/txtPoppinssemibold16" tools:text="@string/msg_course_in_progr"/>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearCoursesList"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_42pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnrectanglethirtysix"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_weight="0.36"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView
                            android:id="@+id/imageRectangleThirtySix"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_70pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_70pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_rectangle36}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle36"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPython"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_13pxh"
                            android:layout_marginStart="@dimen/_13pxh"
                            android:layout_marginTop="@dimen/_28pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_python"
                            style="@style/txtMontserratregular12" tools:text="@string/lbl_python"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearColumnrectanglethirtyeight"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_weight="0.36"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView
                            android:id="@+id/imageRectangleThirtyEight"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_70pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_70pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_rectangle38}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle38"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFlutter"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_22pxh"
                            android:layout_marginStart="@dimen/_22pxh"
                            android:layout_marginTop="@dimen/_27pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_flutter"
                            style="@style/txtMontserratregular12" tools:text="@string/lbl_flutter"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnrectanglethirtynine"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.28"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:gravity="start"
                            android:id="@+id/imageRectangleThirtyNine"
                            android:layout_height="@dimen/_85pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_rectangle39}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle39"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtC"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_31pxh"
                            android:layout_marginStart="@dimen/_31pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_c"
                            style="@style/txtMontserratregular12" tools:text="@string/lbl_c"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameBottombar"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" style="@style/groupStyleblack_900cornerRadius">
                <LinearLayout android:id="@+id/linearRowhome"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_17pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_97pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                    <LinearLayout android:id="@+id/linearRowmarks"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.92"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMarks"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_marks"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_marks"/>
                        <ImageView android:id="@+id/imageLock"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_97pxh"
                            android:layout_marginStart="@dimen/_60pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_lock"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </FrameLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/floatingBtnFloatingactionbutton"
            android:layout_gravity="bottom|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_49pxh"
            app:srcCompat="@drawable/img_contrast" app:tint="@color/white_A700"/>
    </FrameLayout>
</layout>
